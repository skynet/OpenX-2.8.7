<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href="xsl/mdb2_changeset.xsl"?>
<instructionset>
  <name>openads</name>
  <version>538</version>
  <comments>Timezone support</comments>
  <constructive>
    <changeset>
      <name>openads</name>
      <version>538</version>
      <change>
        <table>
          <name>data_intermediate_ad</name>
          <add>
            <field>
              <name>date_time</name>
              <type>openads_datetime</type>
              <notnull>1</notnull>
              <default></default>
              <was>date_time</was>
            </field>
          </add>
          <index>
            <add>
              <name>ad_id_date_time</name>
              <was>ad_id_date_time</was>
              <indexfield>
                <name>ad_id</name>
                <sorting>ascending</sorting>
              </indexfield>
              <indexfield>
                <name>date_time</name>
                <sorting>ascending</sorting>
              </indexfield>
            </add>
              <add>
                <name>zone_id_date_time</name>
                <was>zone_id_date_time</was>
                <indexfield>
                  <name>zone_id</name>
                  <sorting>ascending</sorting>
                </indexfield>
                <indexfield>
                  <name>date_time</name>
                  <sorting>ascending</sorting>
                </indexfield>
              </add>
                <add>
                  <name>date_time</name>
                  <was>date_time</was>
                  <indexfield>
                    <name>date_time</name>
                    <sorting>ascending</sorting>
                  </indexfield>
                </add>
                  <add>
                    <name>interval_start</name>
                    <was>interval_start</was>
                    <indexfield>
                      <name>interval_start</name>
                      <sorting>ascending</sorting>
                    </indexfield>
                  </add>
                </index>
                <index>
                  <remove>
                    <name>data_intermediate_ad_day</name>
                </remove>
                  <remove>
                    <name>data_intermediate_ad_operation_interval_id</name>
                </remove>
                  <remove>
                    <name>data_intermediate_ad_ad_id</name>
                </remove>
                  <remove>
                    <name>data_intermediate_ad_zone_id</name>
                </remove>
              </index>
            </table>
              <table>
                <name>data_summary_ad_hourly</name>
                <add>
                  <field>
                    <name>date_time</name>
                    <type>openads_datetime</type>
                    <notnull>1</notnull>
                    <default></default>
                    <was>date_time</was>
                  </field>
                </add>
                <index>
                  <add>
                    <name>date_time</name>
                    <was>date_time</was>
                    <indexfield>
                      <name>date_time</name>
                      <sorting>ascending</sorting>
                    </indexfield>
                  </add>
                    <add>
                      <name>ad_id_date_time</name>
                      <was>ad_id_date_time</was>
                      <indexfield>
                        <name>ad_id</name>
                        <sorting>ascending</sorting>
                      </indexfield>
                      <indexfield>
                        <name>date_time</name>
                        <sorting>ascending</sorting>
                      </indexfield>
                    </add>
                      <add>
                        <name>zone_id_date_time</name>
                        <was>zone_id_date_time</was>
                        <indexfield>
                          <name>zone_id</name>
                          <sorting>ascending</sorting>
                        </indexfield>
                        <indexfield>
                          <name>date_time</name>
                          <sorting>ascending</sorting>
                        </indexfield>
                      </add>
                    </index>
                    <index>
                      <remove>
                        <name>data_summary_ad_hourly_day</name>
                    </remove>
                      <remove>
                        <name>data_summary_ad_hourly_hour</name>
                    </remove>
                      <remove>
                        <name>data_summary_ad_hourly_ad_id</name>
                    </remove>
                      <remove>
                        <name>data_summary_ad_hourly_zone_id</name>
                    </remove>
                  </index>
                </table>
              </change>
            </changeset>
          </constructive>
          <destructive>
            <changeset>
              <name>openads</name>
              <version>538</version>
              <change>
                <table>
                  <name>data_intermediate_ad</name>
                <remove>
                  <field>
                    <name>day</name>
                  </field>
                    <field>
                      <name>hour</name>
                    </field>
                  </remove>
                </table>
                  <table>
                    <name>data_summary_ad_hourly</name>
                  <remove>
                    <field>
                      <name>day</name>
                    </field>
                      <field>
                        <name>hour</name>
                      </field>
                    </remove>
                  </table>
                </change>
              </changeset>
            </destructive>
          </instructionset>
