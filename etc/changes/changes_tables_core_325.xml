<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href="xsl/mdb2_changeset.xsl"?>
<instructionset>
  <name>openads</name>
  <version>325</version>
  <comments>Add fields required by agency table</comments>
  <constructive>
    <changeset>
      <name>openads</name>
      <version>325</version>
      <change>
          <table>
            <name>agency</name>
            <add>
              <field>
                <name>logout_url</name>
                <type>openads_varchar</type>
                <length>255</length>
                <notnull></notnull>
                <default></default>
                <was>logout_url</was>
              </field>
                <field>
                  <name>active</name>
                  <type>openads_smallint</type>
                  <length>1</length>
                  <notnull></notnull>
                  <default>0</default>
                  <was>active</was>
                </field>
                  <field>
                    <name>updated</name>
                    <type>openads_datetime</type>
                    <notnull>1</notnull>
                    <default></default>
                    <was>updated</was>
                  </field>
                </add>
              </table>
          </change>
        </changeset>
      </constructive>
      <destructive>
        <changeset>
          <name>openads</name>
          <version>325</version>
        </changeset>
      </destructive>
    </instructionset>
