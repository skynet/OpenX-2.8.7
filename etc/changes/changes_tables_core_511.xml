<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href="xsl/mdb2_changeset.xsl"?>
<instructionset>
  <name>openads</name>
  <version>511</version>
  <comments>new fields ad_zone_assoc.priority_factor,  ad_zone_assoc.to_be_delivered, data_summary_ad_zone_assoc.to_be_delivered</comments>
  <constructive>
    <changeset>
      <name>openads</name>
      <version>511</version>
      <change>
        <table>
          <name>ad_zone_assoc</name>
          <add>
            <field>
              <name>priority_factor</name>
              <type>openads_double</type>
              <notnull></notnull>
              <default>0</default>
              <was>priority_factor</was>
            </field>
              <field>
                <name>to_be_delivered</name>
                <type>openads_tinyint</type>
                <length>1</length>
                <notnull>1</notnull>
                <default>1</default>
                <was>to_be_delivered</was>
              </field>
            </add>
          </table>
            <table>
              <name>data_summary_ad_zone_assoc</name>
              <add>
                <field>
                  <name>to_be_delivered</name>
                  <type>openads_tinyint</type>
                  <length>1</length>
                  <notnull>1</notnull>
                  <default>1</default>
                  <was>to_be_delivered</was>
                </field>
              </add>
            </table>
          </change>
        </changeset>
      </constructive>
      <destructive>
        <changeset>
          <name>openads</name>
          <version>511</version>
        </changeset>
      </destructive>
    </instructionset>
