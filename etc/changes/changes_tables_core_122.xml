<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href="xsl/mdb2_changeset.xsl"?>
<instructionset>
  <name>openads</name>
  <version>122</version>
  <comments>The table changed so it now contains all the required columns and unnecessary columns were removed.</comments>
  <constructive>
    <changeset>
      <name>openads</name>
      <version>122</version>
      <change>
        <table>
          <name>clients</name>
          <add>
            <field>
              <name>agencyid</name>
              <type>openads_mediumint</type>
              <length>9</length>
              <notnull>1</notnull>
              <default>0</default>
              <was>agencyid</was>
            </field>
              <field>
                <name>comments</name>
                <type>openads_text</type>
                <notnull></notnull>
                <default></default>
                <was>comments</was>
              </field>
                <field>
                  <name>updated</name>
                  <type>openads_datetime</type>
                  <notnull></notnull>
                  <default></default>
                  <was>updated</was>
                </field>
              </add>
            </table>
          </change>
        </changeset>
      </constructive>
      <destructive>
        <changeset>
          <name>openads</name>
          <version>122</version>
          <change>
            <table>
              <name>clients</name>
            <remove>
              <field>
                <name>views</name>
              </field>
                <field>
                  <name>clicks</name>
                </field>
                  <field>
                    <name>expire</name>
                  </field>
                    <field>
                      <name>activate</name>
                    </field>
                      <field>
                        <name>active</name>
                      </field>
                        <field>
                          <name>weight</name>
                        </field>
                          <field>
                            <name>target</name>
                          </field>
                            <field>
                              <name>parent</name>
                            </field>
                          </remove>
                        </table>
                      </change>
                    </changeset>
                  </destructive>
                </instructionset>
